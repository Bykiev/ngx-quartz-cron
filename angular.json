{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-cron": {
      "projectType": "library",
      "root": "projects/ng-cron",
      "sourceRoot": "projects/ng-cron/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-cron/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-cron/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-cron/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-cron/src/test.ts",
            "tsConfig": "projects/ng-cron/tsconfig.spec.json",
            "karmaConfig": "projects/ng-cron/karma.conf.js"
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "browserTarget": "ng-cron:build",
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "browserTarget": "ng-cron:build"
          }
        }
      }
    }
  }
}
